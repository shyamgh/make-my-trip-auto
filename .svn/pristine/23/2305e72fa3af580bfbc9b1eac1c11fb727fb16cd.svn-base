<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="DCCRegressionSample1" parallel="none" verbose="3" preserve-order="true">
	
  <parameter name="selenium.host" value="localhost" />
  <parameter name="selenium.port" value="3737" />
  <parameter name="selenium.browser" value="*firefox" />
  <parameter name="dcc.url" value="http://www.google.com" />
  <parameter name="dcc.user" value="abc@abc.com" />
  <parameter name="dcc.pass" value="testpass" />
  
  <parameter name="dm.device" value="device" />
  <parameter name="dm.area" value="testarea" />
  <parameter name="dm.group" value="testgroup" />

  <listeners>
  <!--
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
  -->
      <listener class-name="reporter.CustomReporter" />
  </listeners>
  
  <test name="DCCTests" preserve-order="true">
    <classes>
      <class name="tests.DCCTestClass" >
        <methods>
          <include name="DCCUseridPresentTest" />
          <include name="DCCLoginTest" />
          <include name="DCCDMLinkPresentTest" />
          <include name="DCCRegDeviceEndToEndTest" />
          <include name="DCCSelectDeviceTest" />
          <include name="DCCSelectAreaTest" />
          <include name="DCCSelectGroupTest" />          
        </methods>
      </class>
    </classes>
  </test>
  
  <test name="MakeMyTripTests" preserve-order="true">
    <classes>
  <!--
      <class name="tests.TestClassTest" >
        <methods>
	      <include name="LoadHomePageTest" />
          <include name="BusSearchTest" /> 
          <include name="testPrintMessage" />
        </methods>
      </class>

      <class name="tests.UseLogger" >
        <methods>
          <include name="test1" />
        </methods>
      </class>
   -->
      <class name="tests.RawTestClass" >
        <methods>
          <include name="RegisterDeviceTest" />
          <include name="test1" />
        </methods>
      </class>
    </classes>
  </test>
  
</suite>
